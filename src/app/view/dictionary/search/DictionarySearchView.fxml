<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="535.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="bttSearch" mnemonicParsing="false" prefHeight="27.0" prefWidth="196.0" text="Buscar">
               <HBox.margin>
                  <Insets left="1.0" right="2.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
   <center>
      <VBox prefHeight="369.0" prefWidth="535.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="tableDictionary" prefHeight="371.0" prefWidth="535.0">
               <columns>
                  <TableColumn fx:id="columnCod" prefWidth="90.0" text="Cod" />
                  <TableColumn fx:id="columnTitle" prefWidth="264.0" text="Titulo" />
                  <TableColumn fx:id="columnLang" prefWidth="133.0" text="Lingua" />
               </columns>
            </TableView>
         </children>
      </VBox>
   </center>
   <bottom>
      <VBox alignment="CENTER" prefHeight="37.0" prefWidth="500.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="bttSelection" mnemonicParsing="false" prefHeight="25.0" prefWidth="126.0" text="Selecionar" />
         </children>
      </VBox>
   </bottom>
</BorderPane>
